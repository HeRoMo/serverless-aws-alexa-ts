{
  "name": "serverless-aws-alexa-ts",
  "version": "1.0.0",
  "description": "A template of Serverless framework for Alexa skill",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "./node_modules/.bin/serverless deploy",
    "package": "./node_modules/.bin/serverless package",
    "ask:get-model": "ask api get-model -s $npm_config_skill_id -l ja-JP > skill/model_ja-JP.json",
    "ask:update-model": "ask api update-model -s $npm_config_skill_id -l ja-JP -f skill/model_ja-JP.json"
  },
  "dependencies": {
    "ask-sdk": "^2.0.7",
    "source-map-support": "^0.5.0",
    "ssml-builder": "^0.4.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.7",
    "@types/node": "^8.10.21",
    "@types/source-map-support": "^0.4.1",
    "@types/webpack": "^4.4.5",
    "ask-cli": "^1.4.0",
    "serverless": "^1.28.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1"
  },
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/serverless-aws-alexa-ts.git",
  "license": "MIT"
}
